<p id="notice"><%= notice %></p>

<p>
  <b>Number:</b>
  <%= @project.number %>
</p>

<p>
  <b>Name:</b>
  <%= @project.name %>
</p>

<p>
  <b>State:</b>
  <%= @project.state %>
</p>

<p>
  <b>Region:</b>
  <%= @project.region %>
</p>

<p>
  <b>Description:</b>
  <%= @project.description %>
</p>

<p>
  <b>Stakeholders:</b>
  <% @project.stakeholders.each do |stakeholder| %>
  <li>
    <%= link_to stakeholder[:organisation], stakeholder %>
  </li>
  <% end %>
</p>

<%= link_to 'Edit', edit_project_path(@project) %> |
<%= link_to 'Back', projects_path %>
